package follow;

import java.util.Date;

public class FollowVO {

	private int myTeamNo;
	private int followingTeamNo;
	private Date followDate;
	
	private int follower;
	private int following;
	
	
	
	public int getFollower() {
		return follower;
	}
	public void setFollower(int follower) {
		this.follower = follower;
	}
	public int getFollowing() {
		return following;
	}
	public void setFollowing(int following) {
		this.following = following;
	}
	public int getMyTeamNo() {
		return myTeamNo;
	}
	public void setMyTeamNo(int myTeamNo) {
		this.myTeamNo = myTeamNo;
	}
	public int getFollowingTeamNo() {
		return followingTeamNo;
	}
	public void setFollowingTeamNo(int followingTeamNo) {
		this.followingTeamNo = followingTeamNo;
	}
	public Date getFollowDate() {
		return followDate;
	}
	public void setFollowDate(Date followDate) {
		this.followDate = followDate;
	}
	
	
}
